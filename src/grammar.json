{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "brazil_config",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "package"
    },
    "package": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        },
        {
          "type": "FIELD",
          "name": "assignment",
          "content": {
            "type": "STRING",
            "value": "="
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "dictionary"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "_key"
          }
        },
        {
          "type": "FIELD",
          "name": "assignment",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "="
              },
              {
                "type": "STRING",
                "value": "+="
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 3,
          "content": {
            "type": "SYMBOL",
            "name": "keyword"
          }
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "version"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "version"
          }
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "boolean"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "dictionary"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "list"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "build-environment"
        },
        {
          "type": "STRING",
          "value": "build-system"
        },
        {
          "type": "STRING",
          "value": "build-task-configuration"
        },
        {
          "type": "STRING",
          "value": "build-tools"
        },
        {
          "type": "STRING",
          "value": "dependencies"
        },
        {
          "type": "STRING",
          "value": "deploy"
        },
        {
          "type": "STRING",
          "value": "interfaces"
        },
        {
          "type": "STRING",
          "value": "rebuild-when-dependency-changes"
        },
        {
          "type": "STRING",
          "value": "remove-dependencies"
        },
        {
          "type": "STRING",
          "value": "resolves-conflict-dependencies"
        },
        {
          "type": "STRING",
          "value": "runtime-dependencies"
        },
        {
          "type": "STRING",
          "value": "scope"
        },
        {
          "type": "STRING",
          "value": "synonym-for"
        },
        {
          "type": "STRING",
          "value": "targets"
        },
        {
          "type": "STRING",
          "value": "test-dependencies"
        },
        {
          "type": "STRING",
          "value": "type"
        }
      ]
    },
    "dictionary": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "pair"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_elements"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_elements": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_value"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_value"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "_quoted_string"
          }
        },
        {
          "type": "PREC",
          "value": -1,
          "content": {
            "type": "SYMBOL",
            "name": "_non_quoted_string"
          }
        }
      ]
    },
    "_quoted_string": {
      "type": "PATTERN",
      "value": "\"(?:\\\\\"|[^\"])*\""
    },
    "_non_quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "(?:\\\\[\\s#,;{}=+()]|[^\\s#,;{}=+()\"])+"
        }
      }
    },
    "version": {
      "type": "PATTERN",
      "value": "(\\d+(?:\\.[a-zA-Z0-9_\\-]+)*)"
    },
    "boolean": {
      "type": "PATTERN",
      "value": "(true|false)"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -10,
        "content": {
          "type": "PATTERN",
          "value": "#[^\\n]*"
        }
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\\\?\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}