{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "brazil_config",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "package"
    },
    "package": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        },
        {
          "type": "FIELD",
          "name": "assignment",
          "content": {
            "type": "STRING",
            "value": "="
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "dictionary"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        },
        {
          "type": "FIELD",
          "name": "assignment",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "="
              },
              {
                "type": "STRING",
                "value": "+="
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_value"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dictionary"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "version"
          }
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "boolean"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "dictionary": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "pair"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_elements"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_elements": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_value"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_value"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "_quoted_string"
          }
        },
        {
          "type": "PREC",
          "value": 0,
          "content": {
            "type": "SYMBOL",
            "name": "_non_quoted_string"
          }
        }
      ]
    },
    "version": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 3,
          "content": {
            "type": "SYMBOL",
            "name": "_mixed_version"
          }
        },
        {
          "type": "PREC",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "_semver_number"
          }
        },
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "_number"
          }
        }
      ]
    },
    "boolean": {
      "type": "SYMBOL",
      "name": "_boolean"
    },
    "_quoted_string": {
      "type": "PATTERN",
      "value": "\"(?:\\\\\"|[^\"])*\""
    },
    "_non_quoted_string": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "(?:\\\\[\\s#,;{}=+()]|[^\\s#,;{}=+()\"])+"
        }
      }
    },
    "_number": {
      "type": "PATTERN",
      "value": "\"?[0-9]+(\\.[0-9]+)?\"?"
    },
    "_semver_number": {
      "type": "PATTERN",
      "value": "\"?[0-9]+(\\.[0-9]+)*\\.x\"?"
    },
    "_mixed_version": {
      "type": "PATTERN",
      "value": "\"?[0-9]+(\\.[0-9]+)+(-[a-zA-Z0-9]+)?\"?"
    },
    "_boolean": {
      "type": "PATTERN",
      "value": "\"?(true|false)\"?"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -10,
        "content": {
          "type": "PATTERN",
          "value": "#[^\\n]*"
        }
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\\\?\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}